{
  "name": "nextjs-starter",
  "version": "0.0.1",
  "private": true,
  "description": "React, Redux, CSS Modules, server side rendering with NextJS and I18n on board",
  "author": "alexpryshchepa",
  "license": "ISC",
  "engines": {
    "yarn": ">=1.12.3",
    "node": ">=10.14.0",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "lint-js": "eslint --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "test:cypress": "yarn run cypress open",
    "test:jest": "jest",
    "dev": "node server",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@zeit/next-sass": "^1.0.1",
    "express": "^4.16.4",
    "github-markdown-css": "^3.0.1",
    "lodash": "^4.17.11",
    "next": "^7.0.2",
    "next-i18next": "^0.32.0",
    "node-sass": "^4.11.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "reset-css": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "react-test-renderer": "^16.8.2",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run fix"
    }
  }
}
